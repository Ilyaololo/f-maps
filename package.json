    "build": "better-npm-run build",
    "dev": "concurrently \"better-npm-run dev\" \"better-npm-run serve\"",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "serve": "better-npm-run serve"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "WEBPACK_CONFIG": [
          "webpack.config.prod.ts"
        ]
      }
    },
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK_CONFIG": [
          "webpack.config.dev.ts"
        ]
      }
    },
    "serve": {
      "command": "serve --listen $PORT ./public",
      "env": {}
    }
  },
